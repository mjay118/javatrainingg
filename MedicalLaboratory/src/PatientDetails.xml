<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
<class name="com.pojo.Patient" table="patient">
<id name="patient_id" column="patient_id"></id>
<property name="patient_name" column="name"></property>
<property name="patient_gender" column="gender"></property>
<property name="date_of_birth" column="date_of_birth"></property>
<property name="patient_email" column="email"></property>
<property name="patient_password" column="password"></property>
<property name="patient_phone" column="phone"></property>
<property name="patient_address" column="address"></property>
</class>
</hibernate-mapping>